<UserControl x:Class="ExamenSegundoParcial.InformacionPersonal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Background="White">
        <TextBlock FontSize="16" Margin="10,0,0,0">Nombre:</TextBlock>
        <TextBox Name="tbxNombre" Margin="10,0,10,10"></TextBox>
        <TextBlock FontSize="16" Margin="10,0,0,0">Apellido paterno:</TextBlock>
        <TextBox Name="tbxApellidoPaterno" Margin="10,0,10,10"></TextBox>
        <TextBlock FontSize="16" Margin="10,0,0,0">Apellido materno:</TextBlock>
        <TextBox Name="tbxApellidoMaterno" Margin="10,0,10,10"></TextBox>
        <TextBlock FontSize="16" Margin="10,0,0,0">Edad:</TextBlock>
        <TextBox Name="tbxEdad" Margin="10,0,10,10"></TextBox>
    </StackPanel>
</UserControl>
