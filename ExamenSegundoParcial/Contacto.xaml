<UserControl x:Class="ExamenSegundoParcial.Contacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Background="White">
        <TextBlock FontSize="16" Margin="10,0,0,0">Correo:</TextBlock>
        <TextBox Name="tbxCorreo" Margin="10,0,10,10"></TextBox>
        <TextBlock FontSize="16" Margin="10,0,0,0">Telefono:</TextBlock>
        <TextBox Name="tbxTelefono" Margin="10,0,10,10"></TextBox>
        <TextBlock FontSize="16" Margin="10,0,0,0">Celular:</TextBlock>
        <TextBox Name="tbxCelular" Margin="10,0,10,10"></TextBox>
    </StackPanel>
</UserControl>
